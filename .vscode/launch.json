{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run bitwhip",
            "request": "launch",
            "preLaunchTask": "Build bitwhip (Release)",
            "program": "${workspaceFolder}/target/release/bitwhip",
            "windows": {
                "env": {
                    "PATH": "${env:Path};${workspaceFolder}/ext/ffmpeg-n7.0-latest-win64-gpl-shared-7.0/bin"
                }
            },
            "linux": {
                "env": {
                    "LD_LIBRARY_PATH": "${workspaceFolder}/ext/ffmpeg-n7.0-latest-linux64-gpl-shared-7.0/lib"
                },
            },
            "cwd": "${workspaceFolder}",
            "type": "lldb",
            "console": "integratedTerminal",
        },
        {
            "name": "Debug bitwhip",
            "request": "launch",
